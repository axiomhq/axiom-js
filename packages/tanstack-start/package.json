{
  "name": "@axiomhq/tanstack-start",
  "version": "0.1.0",
  "description": "The official TanStack Start and TanStack Router package for Axiom",
  "author": "Axiom, Inc.",
  "license": "MIT",
  "contributors": [
    "Lukas Malkmus <lukas@axiom.co>",
    "Islam Shehata <islam@axiom.co>",
    "Arne Bahlo <arne@axiom.co>",
    "Gabriel De Andrade <gabriel@axiom.co>"
  ],
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./router": {
      "import": {
        "types": "./dist/esm/router.d.ts",
        "default": "./dist/esm/router.js"
      },
      "require": {
        "types": "./dist/cjs/router.d.cts",
        "default": "./dist/cjs/router.cjs"
      }
    },
    "./start": {
      "import": {
        "types": "./dist/esm/start.d.ts",
        "default": "./dist/esm/start.js"
      },
      "require": {
        "types": "./dist/cjs/start.d.cts",
        "default": "./dist/cjs/start.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "vite build",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "test": "vitest run ./test/unit/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axiomhq/axiom-js.git"
  },
  "keywords": [
    "axiom",
    "logging",
    "tanstack",
    "tanstack-router",
    "tanstack-start",
    "react"
  ],
  "homepage": "https://github.com/axiomhq/axiom-js/blob/main/packages/tanstack-start/README.md",
  "engines": {
    "node": ">=20"
  },
  "bugs": {
    "url": "https://github.com/axiomhq/axiom-js/issues"
  },
  "peerDependencies": {
    "@axiomhq/logging": "workspace:*",
    "@tanstack/react-router": ">=1",
    "@tanstack/react-start": ">=1",
    "@tanstack/start-client-core": ">=1"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-start": {
      "optional": true
    },
    "@tanstack/start-client-core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@axiomhq/logging": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@tanstack/config": "^0.16.0",
    "@tanstack/react-router": "^1.162.9",
    "@tanstack/react-start": "^1.162.9",
    "@tanstack/start-client-core": "^1.162.9",
    "vite": "^7.2.6",
    "vitest": "^4.0.14"
  }
}
